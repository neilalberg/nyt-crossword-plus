{
  "manifest_version": 2,
  "name": "New York Times Crossword Plus",
  "version": "0.1.0",
  "description": "An extension for saving NYT Mini Crossword results",
  "content_scripts": [
    {
      "js": [
        "package/jquery-3.3.1.min.js",
        "main.js"
      ],
      "matches": [
        "https://www.nytimes.com/puzzles/leaderboards"
      ]
    }
  ]
}